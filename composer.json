{
    "name": "tyamahori/laravelorbstack",
    "type": "project",
    "description": "Laravel and OrbStack by tyamahori!!",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.5",
        "ext-pdo": "*",
        "cuyz/valinor": "^2.3.2",
        "guzzlehttp/guzzle": "^7.10.0",
        "laravel/framework": "^12.33.0",
        "laravel/tinker": "^2.11.1",
        "league/flysystem-aws-s3-v3": "^3.32.0",
        "open-telemetry/opentelemetry": "^1.0",
        "php-http/httplug": "^2.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^4.0.10",
        "barryvdh/laravel-ide-helper": "^3.6.1",
        "deptrac/deptrac": "^4.6.0",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.9.3",
        "nunomaduro/collision": "^8.9.1",
        "php-standard-library/phpstan-extension": "^2.0.2",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan-doctrine": "^2.0.18",
        "phpstan/phpstan-nette": "^2.0.9",
        "phpstan/phpstan-phpunit": "^2.0.16",
        "phpstan/phpstan-strict-rules": "^2.0.10",
        "phpunit/phpunit": "^12.5.14",
        "rector/rector": "^2.3.8",
        "roave/security-advisories": "dev-latest",
        "symplify/easy-coding-standard": "^13.0.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelOrbStack\\": "packages/",
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate .tempCache/.ide_helper.php",
            "@php artisan ide-helper:models --nowrite --dir=./packages --dir=./app -F .tempCache/.model.php",
            "@php artisan ide-helper:meta -F .tempCache/.meta.php"
        ],
        "stanCheck": [
            "./vendor/bin/phpstan analyse -c ./libConfig/phpstan.neon"
        ],
        "ecsCheck": [
            "./vendor/bin/ecs check -c ./libConfig/ecs.php"
        ],
        "rectorCheck": [
            "./vendor/bin/rector process --dry-run -c ./libConfig/rector.php"
        ],
        "phpunit": [
            "./vendor/bin/phpunit -c ./libConfig/phpunit.xml"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        },
        "platform": {
            "php": "8.5"
        },
        "nextPhp": "8.6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
