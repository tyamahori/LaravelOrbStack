{
    "name": "tyamahori/laravelorbstack",
    "type": "project",
    "description": "Laravel and OrbStack by tyamahori!!",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.4.4",
        "ext-pdo": "*",
        "cuyz/valinor": "^1.14.4",
        "guzzlehttp/guzzle": "^7.9.2",
        "laravel/framework": "^12.0.1",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.29.0",
        "open-telemetry/opentelemetry": "^1.0",
        "php-http/httplug": "^2.4.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15.2",
        "barryvdh/laravel-ide-helper": "^3.5.5",
        "deptrac/deptrac": "^3.0",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.1.0",
        "php-standard-library/phpstan-extension": "^2.0.0",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan-doctrine": "^2.0.2",
        "phpstan/phpstan-nette": "^2.0.3",
        "phpstan/phpstan-phpunit": "^2.0.4",
        "phpstan/phpstan-strict-rules": "^2.0.3",
        "phpunit/phpunit": "^12.0.7",
        "rector/rector": "^2.0.10",
        "roave/security-advisories": "dev-latest",
        "symplify/easy-coding-standard": "^12.5.8"
    },
    "autoload": {
        "psr-4": {
            "LaravelOrbStack\\": "packages/",
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate .tempCache/.ide_helper.php",
            "@php artisan ide-helper:models --nowrite --dir=./packages --dir=./app -F .tempCache/.model.php",
            "@php artisan ide-helper:meta -F .tempCache/.meta.php"
        ]

    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
